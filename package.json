{
	"name": "voutline",
	"displayName": "voutline",
	"description": "visual outline",
	"version": "0.4.0",
	"publisher": "HsinYuanYeh",
	"repository": {
		"type": "git",
		"url": "https://github.com/iapyeh/voutline.git"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.start",
		"onCommand:extension.insert_timestamp"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.start",
				"title": "Voutline: Visual Outline"
			},
			{
				"command": "extension.insert_timestamp",
				"title": "Voutline: insert timestamp"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
